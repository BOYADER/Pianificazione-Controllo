<launch>
	<rosparam file = "$(find modellazione)/config/mission.yaml"/>
	<rosparam file = "$(find modellazione)/config/constant.yaml"/>
	<node name ="model" pkg="modellazione" type="model"/>
	<rosparam file="$(find pc_wp)/config/mission.yaml" />
	<rosparam file="$(find pc_wp)/config/rospy.yaml" />
	<rosparam file="$(find pc_wp)/config/task_manager.yaml" />
	<rosparam file="$(find pc_wp)/config/frame.yaml" />
	<rosparam file="$(find pc_wp)/config/pid.yaml" />
	<rosparam file="$(find pc_wp)/config/references.yaml" />
	<node pkg="pc_wp" type="navigation.py" name="navigation" />

	<node pkg="pc_wp" type="pitch_task.py" name="pitch_task"/>
	<node pkg="pc_wp" type="yaw_task.py" name="yaw_task"/>

	<node pkg="pc_wp" type="surge_task.py" name="surge_task"/>

	<node pkg="pc_wp" type="task_manager.py" name="task_manager"/>

	<node pkg="pc_wp" type="approach_task.py" name="approach_task"/>

	<node pkg="pc_wp" type="heave_task.py" name="heave_task"/>
</launch>
